<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: white; -fx-border-radius: 2; -fx-border-color: black; -fx-border-insets: 2;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.reportMenuController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Main Menu">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <VBox layoutX="45.0" layoutY="80.0" prefHeight="240.0" prefWidth="210.0" spacing="50.0">
            <children>
                <Button mnemonicParsing="false" onAction="#totalByCustBtnClick" prefHeight="50.0" prefWidth="233.0" text="Total Appointments by Customer">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button maxHeight="-Infinity" mnemonicParsing="false" onAction="#contactScheduleBtnClick" prefHeight="60.0" prefWidth="210.0" text="Contact Schedule">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Button>
                <Button fx:id="reports" mnemonicParsing="false" onAction="#totalByCountryBtnClick" prefHeight="50.0" prefWidth="210.0" text="Total Appointments by Country">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#mainMenuBtnClick" prefHeight="50.0" prefWidth="210.0" text="Main Menu">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
